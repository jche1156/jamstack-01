{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@globals": [
        "src/components/global"
      ],
      "@component/*": [
        "src/components/*"
      ],
      "@layout/*": [
        "src/layout/*"
      ],
      "@style/*": [
        "src/styles/*"
      ],
      "@util/*": [
        "utils/*"
      ],
      "@img/*": [
        "src/assets/img/*"
      ],
      "@svg/*": [
        "src/assets/svg/*"
      ],
      "@icon/*": [
        "src/assets/svg/icon/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}